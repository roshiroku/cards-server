{
  "env": "development",
  "db": {
    "provider": "mongodb"
  },
  "auth": {
    "provider": "jwt"
  },
  "validation": {
    "provider": "joi"
  },
  "logging": {
    "provider": "morgan",
    "format": "custom",
    "colors": {
      "success": "cyanBright",
      "error": "redBright"
    }
  },
  "cors": {
    "origin": [
      "http://127.0.0.1:5500",
      "http://localhost:5500",
      "http://localhost:5173"
    ]
  }
}